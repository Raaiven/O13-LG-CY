        <script>
    var swiper1 = new Swiper('.swiper1', {
      watchSlidesVisibility: true,
      slidesPerView: 3,
      observer: true,
      observeParents: true,
      spaceBetween: 30,
      centeredSlides: true,
      pagination: {
        el: '.swiper-pagination',
        clickable: true,
      },
    
      navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev',
      },
       breakpoints: {
    1: {
      slidesPerView: 1,
    },
    680: {
      slidesPerView: 3,
      spaceBetween: 30
    },
  }
    
    });
    
        

    var d1j = document.getElementById("3b_div1");
    var d2j = document.getElementById("3b_div2");
    var d3j = document.getElementById("3b_div3");
    var d4j = document.getElementById("3b_div4");
    var d5j = document.getElementById("3b_div5");  
    swiper1.on('transitionEnd ', function () {
        console.log('*** swiper1.realIndex', swiper1.realIndex);
        if (swiper1.realIndex == 0) {
          if (d1j.style.display === "none") {
            d1j.style.display = "block";
            d2j.style.display = "none";
            d3j.style.display = "none";
            d4j.style.display = "none";
            d5j.style.display = "none";
          }
        
        }
        if (swiper1.realIndex == 1) {
          if (d2j.style.display === "none") {
            d1j.style.display = "none";
            d2j.style.display = "block";
            d3j.style.display = "none";
            d4j.style.display = "none";
            d5j.style.display = "none";
          }
        
        }
        if (swiper1.realIndex == 2) {
          if (d3j.style.display === "none") {
            d1j.style.display = "none";
            d2j.style.display = "none";
            d3j.style.display = "block";
            d4j.style.display = "none";
            d5j.style.display = "none";
          }
        
        }
        if (swiper1.realIndex == 3) {
          if (d4j.style.display === "none") {
            d1j.style.display = "none";
            d2j.style.display = "none";
            d3j.style.display = "none";
            d4j.style.display = "block";
            d5j.style.display = "none";
          }
        
        }
        if (swiper1.realIndex == 4) {
          if (d5j.style.display === "none") {
            d1j.style.display = "none";
            d2j.style.display = "none";
            d3j.style.display = "none";
            d4j.style.display = "none";
            d5j.style.display = "block";
          }
        
        }        
    });
        </script>    
    
